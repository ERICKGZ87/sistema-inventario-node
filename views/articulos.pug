
extends ./layout/index

block contenido
    h1.mt-3 lista de productos

    table.table.mt-3
        thead
            tr
         
            th(scope="col") id
            th(scope="col") nombre
            th(scope="col") stok
        tbody
        each articulos in articulosLista
            tr
            td= articulos.id
            td= articulos.nombre
            td= articulos.stock